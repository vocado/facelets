<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib xmlns="http://java.sun.com/JSF/Facelet">
  <namespace>http://xmlns.oracle.com/adf/faces/html</namespace>

  <tag>
    <tag-name>rowLayout</tag-name>
    <component>
      <component-type>oracle.adf.HtmlRowLayout</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>styleSheet</tag-name>
    <component>
      <component-type>oracle.adf.CoreStyleSheet</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>tableLayout</tag-name>
    <component>
      <component-type>oracle.adf.HtmlTableLayout</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>frame</tag-name>
    <component>
      <component-type>oracle.adf.HtmlFrame</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>html</tag-name>
    <component>
      <component-type>oracle.adf.HtmlHtml</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>body</tag-name>
    <component>
      <component-type>oracle.adf.HtmlBody</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>script</tag-name>
    <component>
      <component-type>oracle.adf.HtmlScript</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>frameBorderLayout</tag-name>
    <component>
      <component-type>oracle.adf.HtmlFrameBorderLayout</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>head</tag-name>
    <component>
      <component-type>oracle.adf.HtmlHead</component-type>
    </component>
  </tag>

  <tag>
    <tag-name>cellFormat</tag-name>
    <component>
      <component-type>oracle.adf.HtmlCellFormat</component-type>
    </component>
  </tag>

</facelet-taglib>
